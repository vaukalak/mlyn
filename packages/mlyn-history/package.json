    "build": "npx babel src --extensions '.ts' --out-dir lib && yarn ts",
    "prepublish": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "mlyn": "^0.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",

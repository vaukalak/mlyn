    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "mlyn": "^0.5.13",
    "typescript": "^4.5.4"
  }
}

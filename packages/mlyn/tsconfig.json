{
  "compilerOptions": {
    "downlevelIteration": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "declaration": true,
    "declarationDir": "./types",
    "emitDeclarationOnly": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "dist",
    "rootDir": "src"
  },
  "include": ["src/**/*"]
}

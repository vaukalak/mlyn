{
  "name": "react-mlyn",
  "version": "0.5.22",
  "description": "React bindings to mlyn",
  "main": "lib/index.js",
  "types": "types/index.js",
  "repository": "https://github.com/vaukalak/react-mlyn",
  "author": "vaukalak",
  "license": "MIT",
  "scripts": {
    "ts": "npx tsc --rootDir src --declarationDir types --downlevelIteration --declaration --emitDeclarationOnly",
    "test": "npx jest",
    "clean": "rm -rf dist",
    "build": "npx babel src --extensions '.ts,.tsx' --out-dir lib && yarn ts",
    "prepublish": "yarn clean && yarn build"
  },
  "files": [
    "lib",
    "types",
    "src"
  ],
  "jest": {
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.0.3",
    "babel-polyfill": "^6.26.0",
    "jest": "^27.0.6",
    "mlyn": "^0.5.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "mlyn": "^0.5.2",
    "react": "^17.0.2"
  },
  "gitHead": "96a6c41df24c53c48f9f72573770babf94bd2dec"
}
